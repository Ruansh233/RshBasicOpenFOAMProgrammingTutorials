FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      h;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{

    topBot
    {
        type            empty;
    }

    north
    {
        type fixedValue;
        value uniform 0;
    }

    south
    {
        type fixedValue;
        value uniform 0;
    }

    east
    {
        type fixedValue;
        value uniform 0;
    }

    west
    {
        type fixedValue;
        value uniform 0;
    }

}

